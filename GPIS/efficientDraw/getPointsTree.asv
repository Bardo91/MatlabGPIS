function [ points, col ] = getPointsTree( points, col, cell )

    if(cell{1} == 2)
       if(length(cell{2}) == 0)
              points = [points; cell{5}];
              if(cell{6} > 0)
                 col = [col; +1]; 
              else
                 col = [col;-1];
              end
       else
           [points, col] = getPointsTree
       end
    end

end

